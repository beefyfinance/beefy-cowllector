        "@types/jest": "^29.5.3",
        "@types/lodash": "^4.14.196",
        "@types/node": "^20.4.9",
        "@types/yargs": "^17.0.24",
        "husky": "^8.0.3",
        "jest": "^29.6.2",
        "lint-staged": "^13.2.3",
        "npm-check-updates": "^16.10.18",
        "prettier": "^3.0.1",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "ts-unused-exports": "^9.0.5",
        "typescript": "^5.1.6"
